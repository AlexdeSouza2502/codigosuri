package uri1078;


import java.util.Scanner;
 
public class Uri1078 {

    
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        int N, MULT;

        do {
            N = sc.nextInt();
        } while (N < 2 && N > 1000);

        for (int i = 1; i <= 10; i++) {
            MULT = i * N;
            System.out.printf("%d x %d = %d\n", i, N, MULT);
        }

    }
}
